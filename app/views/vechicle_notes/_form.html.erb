<%= simple_form_for(@vechicle_note) do |f| %>
  <%= f.error_notification %>

  <div class="form-inputs">
    <%= f.input :name %>
    <%= f.association :user %>
    <%= f.association :vechicle %>
  </div>

  <div class="form-actions">
    <%= f.button :submit %>
    <%= link_to 'Back', vechicle_notes_path %>
  </div>
<% end %>
